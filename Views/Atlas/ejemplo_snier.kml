<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <name>Ejemplo KML SNIER</name>
    <Placemark>
      <name>Punto de ejemplo</name>
      <description>Este es un punto de ejemplo para probar la carga de KML en el visor AZEL.</description>
      <Point>
        <coordinates>-99.1332,19.4326,0</coordinates>
      </Point>
    </Placemark>
    <Placemark>
      <name>Polígono de ejemplo</name>
      <description>Polígono de prueba en la CDMX.</description>
      <Polygon>
        <outerBoundaryIs>
          <LinearRing>
            <coordinates>
              -99.15,19.44,0
              -99.12,19.44,0
              -99.12,19.42,0
              -99.15,19.42,0
              -99.15,19.44,0
            </coordinates>
          </LinearRing>
        </outerBoundaryIs>
      </Polygon>
    </Placemark>
  </Document>
</kml>
